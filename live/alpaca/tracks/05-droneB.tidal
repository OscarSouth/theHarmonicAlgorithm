bpm = 110

bars = 2

juno f s r d = p "juno" $ do
  let o = ch 16
  f
    $ stack [silence
       -- --
      ,arrange flow (overlapF 0 s) r (-9,9) ["~"
        ,"[0 1 2 1]"
      ]# o |* vel 0.7
       -- --
      ,arrange flow (overlapF 0 s) r (-9,9) ["~"
        -- ,"[-6]"
        -- ,"<[~ 6] ~@3>" |+ irand 3
        -- ,"[6]/4" |+ irand 3
      ]# o |* vel 1 #legato 2
       -- --
    ] |* vel d
